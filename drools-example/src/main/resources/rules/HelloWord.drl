package rules;
import com.noogel.xyz.entity.User;
import java.lang.String;
import java.util.List;

rule "hello,word"
    when
        eval(true)
    then
        System.err.println("hello,word!");
end

rule "user"
    when
      $u:User(name=="张三", age==18)
    then
        $u.setName("李四");
        System.err.println("hello,word："+$u.getName());
end